ction createGui(cube) {\r\n  // 创建gui\r\n  const gui = new dat.GUI();\r\n  gui\r\n    .add(cube.position, \"x\")\r\n    .min(0)\r\n    .max(5)\r\n    .step(0.1)\r\n    .name(\"cube X轴\")\r\n    .onChange((value) => {})\r\n    .onFinishChange((value) => {});\r\n  let params = {\r\n    color: \"#ff0000\",\r\n    fn() {\r\n      gsap.to(cube.position, {\r\n        x: 5,\r\n        duration: 3,\r\n        // 重复次数 -1为无限次\r\n        repeat: -1,\r\n        // 往返\r\n        yoyo: true,\r\n        // 延迟\r\n        ease: \"powerl.inOut\",\r\n      });\r\n    },\r\n  };\r\n  gui.addColor(params, \"color\").onChange((value) => {\r\n    cube.material.color.set(value);\r\n  });\r\n  gui.add(cube, \"visible\");\r\n  gui.add(params,\"fn\").name(\"运动\")\r\n  // 创建一个文件夹\r\n  let folder = gui.addFolder(\"设置立方体\")\r\n  folder.add(cube.material,\"wireframe\")\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"28cb1b857416d7c4cdf3eca8e2f56534","cacheData":{"env":{}}}